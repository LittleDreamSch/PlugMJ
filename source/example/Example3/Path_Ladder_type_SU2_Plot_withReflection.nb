      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"coordloop", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"List", "@@", "newloop"}], ")"}], "//", 
       "EdgesToCoordinates2D"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "define", " ", "edge", " ", "as", " ", "a", " ", "pair", " ", "of", " ",
        "coordinates"}], " ", "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"coordloop2", "=", 
      RowBox[{
       RowBox[{"Partition", "[", 
        RowBox[{"coordloop", ",", "2", ",", "1"}], "]"}], "~", "Join", "~", 
       RowBox[{"{", 
        RowBox[{"{", 
