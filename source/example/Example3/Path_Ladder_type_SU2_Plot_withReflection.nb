Cell[BoxData[
 RowBox[{
  RowBox[{"SDEofaddPlaquette", "[", 
   RowBox[{"loop_WL", ",", "linkNum_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"linktype", ",", "ortholinks"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"linktype", "=", 
      RowBox[{"loop", "[", 
       RowBox[{"[", "linkNum", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"find", " ", "all", " ", "orthogonal", " ", "links"}], " ", 
      "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"ortholinks", "=", 
      RowBox[{"Complement", "[", 
       RowBox[{
        RowBox[{"List", "@@", "loop"}], ",", 
        RowBox[{"{", 
         RowBox[{"linktype", ",", 
          RowBox[{"-", "linktype"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"add", " ", "plaquette"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Plus", "@@", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"ReplacePart", "[", 
            RowBox[{"loop", ",", 
             RowBox[{"linkNum", "->", 
              RowBox[{"Sequence", "[", 
               RowBox[{"linktype", ",", "#", ",", 
                RowBox[{"-", "linktype"}], ",", 
                RowBox[{"-", "#"}], ",", "linktype"}], "]"}]}]}], "]"}], "-", 
           
           RowBox[{"ReplacePart", "[", 
            RowBox[{"loop", ",", 
             RowBox[{"linkNum", "->", 
              RowBox[{"Sequence", "[", 
               RowBox[{"#", ",", "linktype", ",", 
                RowBox[{"-", "#"}]}], "]"}]}]}], "]"}]}], ")"}], "&"}], "/@", 
        "ortholinks"}], ")"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

