Cell[BoxData[
 RowBox[{
  RowBox[{"countRepeatLegs", "[", 
   RowBox[{"loop_WL", ",", "linkNum_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "newloop", ",", "coordloop", ",", "coordloop2", ",", "repeatlinks1", ",",
       "repeatlinks2"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "change", " ", "the", " ", "given", " ", "edge", " ", "to", " ", "first",
       " ", "position"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"newloop", "=", 
      RowBox[{"RotateLeft", "[", 
       RowBox[{"loop", ",", 
