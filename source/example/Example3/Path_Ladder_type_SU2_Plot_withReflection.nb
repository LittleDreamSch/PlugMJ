Cell[BoxData[
 RowBox[{
  RowBox[{"EdgesToCoordinates2D", "[", "edges_List", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"pos", "=", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}]}], ",", 
      RowBox[{"coords", "=", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], "}"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Scan", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"pos", "=", 
           RowBox[{"pos", "+", 
            RowBox[{"Switch", "[", 
             RowBox[{"#", ",", "a", ",", 
              RowBox[{"{", 
               RowBox[{"1", ",", "0"}], "}"}], ",", "b", ",", 
              RowBox[{"{", 
               RowBox[{"0", ",", "1"}], "}"}], ",", 
              RowBox[{"-", "a"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"-", "1"}], ",", "0"}], "}"}], ",", 
              RowBox[{"-", "b"}], ",", 
              RowBox[{"{", 
               RowBox[{"0", ",", 
                RowBox[{"-", "1"}]}], "}"}]}], "]"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"coords", ",", "pos"}], "]"}]}], ")"}], "&"}], ",", 
       "edges"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Last", "[", "coords", "]"}], "===", 
        RowBox[{"First", "[", "coords", "]"}]}], ",", 
       RowBox[{"Drop", "[", 
        RowBox[{"coords", ",", 
         RowBox[{"-", "1"}]}], "]"}], ",", "\"\<NonClosedLoop\>\""}], 
      "]"}]}]}], "]"}]}]], "Input",
